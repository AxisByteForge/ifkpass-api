{
  "name": "ifkpass-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:dev": "tsup --config tsup.dev.config.ts",
    "build:prod": "tsup --config tsup.prod.config.ts",
    "lint": "eslint src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lambda:local": "npm run build:dev && npx tsx scripts/local-lambda.ts",
    "lambda:create-user": "npm run lambda:local .vscode/payloads/create-user-payload.json",
    "lambda:create-admin": "npm run lambda:local .vscode/payloads/create-admin-payload.json",
    "lambda:verify-email": "npm run lambda:local .vscode/payloads/verify-email-payload.json",
    "lambda:create-profile": "npm run lambda:local .vscode/payloads/create-profile-payload.json",
    "lambda:send-photo": "npm run lambda:local .vscode/payloads/send-photo-payload.json",
    "lambda:authenticate": "npm run lambda:local .vscode/payloads/authenticate-payload.json",
    "lambda:reset-password": "npm run lambda:local .vscode/payloads/reset-password-payload.json",
    "lambda:approve-user": "npm run lambda:local .vscode/payloads/approve-user-payload.json",
    "lambda:pay-card": "npm run lambda:local .vscode/payloads/pay-card-payload.json",
    "lambda:mercado-pago-webhook": "npm run lambda:local .vscode/payloads/mercado-pago-webhook-payload.json",
    "cdk:synth": "cdk synth",
    "cdk:synth:dev": "cdk synth --context stage=dev",
    "cdk:synth:prd": "cdk synth --context stage=prd",
    "cdk:deploy:dev": "cdk deploy --context stage=dev --all",
    "cdk:deploy:prd": "cdk deploy --context stage=prd --all",
    "cdk:destroy:dev": "cdk destroy --context stage=dev",
    "cdk:destroy:prd": "cdk destroy --context stage=prd",
    "cdk:diff:dev": "cdk diff --context stage=dev",
    "cdk:diff:prd": "cdk diff --context stage=prd",
    "cdk:bootstrap": "cdk bootstrap aws://972210179301/us-east-1"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^22.18.13",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "aws-cdk": "^2.1031.0",
    "aws-cdk-lib": "^2.221.1",
    "constructs": "^10.4.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-prettier": "^5.2.3",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.24.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.828.0",
    "@aws-sdk/client-dynamodb": "^3.799.0",
    "@aws-sdk/client-s3": "^3.839.0",
    "@aws-sdk/client-ses": "^3.821.0",
    "@aws-sdk/s3-request-presigner": "^3.835.0",
    "@aws-sdk/util-dynamodb": "^3.808.0",
    "@smithy/smithy-client": "^4.2.1",
    "argon2": "^0.43.0",
    "aws-lambda": "^1.0.7",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "jose": "^6.0.11",
    "pino": "^9.9.0",
    "resend": "^4.5.1",
    "unplugin-swc": "^1.5.1",
    "zod": "^3.24.2"
  }
}
