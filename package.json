{
  "name": "ifkpass-api",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:dev": "tsup --config tsup.dev.config.ts",
    "build:prod": "tsup --config tsup.prod.config.ts",
    "updates": "npx npm-check-updates --target minor -u && npm install",
    "lint": "npx @biomejs/biome format --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "lambda:local": "npm run build:dev && npx tsx scripts/local-lambda.ts",
    "lambda:create-user": "npm run lambda:local .vscode/payloads/create-user-payload.json",
    "lambda:create-admin": "npm run lambda:local .vscode/payloads/create-admin-payload.json",
    "lambda:verify-email": "npm run lambda:local .vscode/payloads/verify-email-payload.json",
    "lambda:create-profile": "npm run lambda:local .vscode/payloads/create-profile-payload.json",
    "lambda:send-photo": "npm run lambda:local .vscode/payloads/send-photo-payload.json",
    "lambda:authenticate": "npm run lambda:local .vscode/payloads/authenticate-payload.json",
    "lambda:reset-password": "npm run lambda:local .vscode/payloads/reset-password-payload.json",
    "lambda:approve-user": "npm run lambda:local .vscode/payloads/approve-user-payload.json",
    "lambda:pay-card": "npm run lambda:local .vscode/payloads/pay-card-payload.json",
    "lambda:mercado-pago-webhook": "npm run lambda:local .vscode/payloads/mercado-pago-webhook-payload.json",
    "cdk:synth": "cdk synth",
    "cdk:deploy": "cdk deploy --all",
    "cdk:destroy": "cdk destroy --all",
    "cdk:diff": "cdk diff"
  },
  "engines": {
    "node": ">=24",
    "npm": ">=11"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/aws-lambda": "8.10.160",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "22.19.7",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "aws-cdk": "2.1104.0",
    "aws-cdk-lib": "2.236.0",
    "constructs": "10.4.5",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "@biomejs/biome": "2.3.13"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "3.978.0",
    "@aws-sdk/client-dynamodb": "3.978.0",
    "@aws-sdk/client-s3": "3.978.0",
    "@aws-sdk/client-ses": "3.978.0",
    "@aws-sdk/s3-request-presigner": "3.978.0",
    "@aws-sdk/util-dynamodb": "3.978.0",
    "@smithy/smithy-client": "4.11.1",
    "argon2": "0.44.0",
    "aws-lambda": "1.0.7",
    "bcrypt": "5.1.1",
    "dotenv": "16.6.1",
    "drizzle-kit": "0.31.8",
    "drizzle-orm": "0.45.1",
    "jose": "6.1.3",
    "jsonwebtoken": "9.0.3",
    "pino": "9.14.0",
    "postgres": "3.4.8",
    "resend": "4.8.0",
    "unplugin-swc": "1.5.9",
    "zod": "3.25.76"
  }
}
